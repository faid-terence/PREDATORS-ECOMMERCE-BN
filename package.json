{
  "name": "ecommerce-app-predators-bn",
  "version": "1.0.0",
  "description": "predator ecommerce project",
  "main": "server.js",
  "type": "module",
  "engines": {
    "node": ">=14.0.0"
  },
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "make-model": "npx sequelize-cli model:generate",
    "migrate": "npx sequelize-cli db:migrate",
    "make-seed": "npx sequelize-cli seed:generate --name",
    "seed": "npx sequelize-cli db:seed:all",
    "_seed": "npx sequelize-cli db:seed --seed",
    "unseed": "npx sequelize-cli db:seed:undo:all",
    "_unseed": "npx sequelize-cli db:seed:undo --seed",
    "down": "npx sequelize-cli db:migrate:undo:all",
    "_down": "npx sequelize-cli db:migrate:undo --to",
    "test": "cross-env NODE_ENV=test mocha --config ./tests/.mocharc.cjs",
    "start": "cross-env NODE_ENV=production node src/server.js",
    "dev": "cross-env NODE_ENV=development nodemon --experimental-modules --es-module-specifier-resolution=node src/server.js",
    "build": "babel src -d dist",
    "deploy": "cross-env NODE_ENV=production babel-node src/server.js",
    "coverage": "c8  --exclude 'config/*.js' --exclude 'services/*.services.js' --exclude 'src/database/**/*.js' --reporter=text-lcov npm run test | coveralls ",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlp-rwanda/ecommerce-app-predators-bn.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atlp-rwanda/ecommerce-app-predators-bn/issues"
  },
  "homepage": "https://github.com/atlp-rwanda/ecommerce-app-predators-bn#readme",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "c8": "^7.14.0",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "config": "^3.3.9",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "cron": "^2.3.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "ejs": "^3.1.9",
    "eslint-config-prettier": "^8.8.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "flush-cache": "^1.0.1",
    "i18next": "^22.4.13",
    "i18next-express-middleware": "^2.0.0",
    "i18next-fs-backend": "^2.1.1",
    "i18next-http-middleware": "^3.3.0",
    "i18next-node-fs-backend": "^2.1.3",
    "iconv-lite": "^0.6.3",
    "joi": "^17.9.1",
    "jsend": "^1.1.0",
    "jsonwebtoken": "^9.0.0",
    "mocha": "^10.2.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.1",
